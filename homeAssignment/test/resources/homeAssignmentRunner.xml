<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SeleniumTestSuite">

    <test name="demoTest">
        <classes>
            <class name="DemoTest"/>
        </classes>
    </test>

    <test name="Register Account">
        <classes>
            <class name="tests.RegisterTest"/>
        </classes>
    </test>

    <test name="Log into Account">
        <classes>
            <class name="tests.LoginTest"/>
        </classes>
    </test>

    <test name="Failed log into Account">
        <classes>
            <class name="tests.FailedLoginTest"/>
        </classes>
    </test>

    <test name="Log Out from Account">
        <classes>
            <class name="tests.LogoutTest"/>
        </classes>
    </test>

    <test name="Transfer founds between accounts">
        <classes>
            <class name="tests.TransferFundsTest"/>
        </classes>
    </test>

    <test name="Pay Bill">
        <classes>
            <class name="tests.BillPayTest"/>
        </classes>
    </test>

    <test name="View transaction history">
        <classes>
            <class name="tests.ViewTransactionsHistoryTest"/>
        </classes>
    </test>

    <test name="Personal Info Update">
        <classes>
            <class name="tests.PersonalInfoUpdateTest"/>
        </classes>
    </test>

    <test name="Open new account">
        <classes>
            <class name="tests.OpenNewAccountTest"/>
        </classes>
    </test>

    <test name="Forgot Login Info Test">
        <classes>
            <class name="tests.ForgotLoginInfoTest"/>
        </classes>
    </test>

</suite>